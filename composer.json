{
    "name": "caryley/laravel-inventory",
    "type": "project",
    "description": "Inventory feature for laravel models.",
    "keywords": [
        "Laravel",
        "Laravel Inventory"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tal Elmishali",
            "email": "tal@caryley.dev",
            "homepage": "https://caryley.dev"
        }
    ],
    "homepage": "https://github.com/caryley/laravel-inventory",
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.22",
        "laravel/pint": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Caryley\\LaravelInventory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caryley\\LaravelInventory\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caryley\\LaravelInventory\\LaravelInventoryServiceProvider"
            ],
            "aliases": {
                "LaravelInventory": "Caryley\\LaravelInventory\\Facades\\LaravelInventory"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
